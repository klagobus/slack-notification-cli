{
  "name": "slack-notification-cli",
  "description": "A new CLI generated with oclif",
  "version": "0.0.0",
  "author": "Karen Lagonell-Busch",
  "bin": {
    "notify": "./bin/run.js"
  },
  "bugs": "https://github.com/slack-notification-cli/issues",
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "@slack/types": "^2.14.0",
    "@slack/web-api": "^7.8.0"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/jest": "^29.5.14",
    "@types/node": "^18",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "jest": "^29.7.0",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/slack-notification-cli/",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "bin": "tests-notify",
    "dirname": "tests-notify",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "send": {
        "description": "Send a notification to Slack"
      }
    }
  },

  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "cli": "node ./bin/run send --token $SLACK_TOKEN",
    "test": "jest",
    "version": "oclif readme && git add README.md",
    "dev:cli": "npm run build && npm run cli"
  },
  "types": "dist/index.d.ts"
}

